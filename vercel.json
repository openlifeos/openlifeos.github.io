{
  "version": 2,
  "alias": ["openlifeos.ai", "www.openlifeos.ai"],
  "routes": [
    {
      "src": "/docs/?$",
      "dest": "/docs/index.html"
    },
    {
      "src": "/docs/(.*)",
      "dest": "/docs/$1"
    },
    {
      "src": "/demo/?$",
      "dest": "/demos/presentation-final.html"
    },
    {
      "src": "/playground/?$",
      "dest": "/demos/life-os-complete.html"
    },
    {
      "src": "/api/?$",
      "dest": "/docs/API-REFERENCE.md"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}